  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash

set -ex

# Export all environment variables needed.
export PULL_BASE_REF=${ghprbTargetBranch}
export PULL_NUMBER=${ghprbPullId}
export ghprbGhRepository
export DEBUG=true

curl -OL https://raw.githubusercontent.com/kata-containers/tests/main/.ci/ci_clh_entry_point.sh
bash ci_clh_entry_point.sh</command>
      <configuredLocalRules/>
